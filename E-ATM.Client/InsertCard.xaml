<Window x:Class="E_ATM.Client.InsertCard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="InsertCard" Height="210" Width="412">
    <Grid Margin="0,0,2,4">
        <TextBox x:Name="txt_first4" HorizontalAlignment="Left" Height="29" Margin="35,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="72" MaxLength="4"/>
        <TextBox x:Name="txt_second4" HorizontalAlignment="Left" Height="29" Margin="123,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="72" MaxLength="4"/>
        <TextBox x:Name="txt_third4" HorizontalAlignment="Left" Height="29" Margin="209,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="72" MaxLength="4"/>
        <TextBox x:Name="txt_last4" HorizontalAlignment="Left" Height="29" Margin="297,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="72" MaxLength="4"/>
        <Button x:Name="btn_Enter" Content="Enter" HorizontalAlignment="Left" Height="33" Margin="54,112,0,0" VerticalAlignment="Top" Width="288" Click="btn_Enter_Click"/>
        <Label Content="Insert your 16 digit card number:" HorizontalAlignment="Left" Height="31" Margin="35,19,0,0" VerticalAlignment="Top" Width="318"/>

    </Grid>
</Window>
